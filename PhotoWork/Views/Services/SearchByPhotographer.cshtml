@model IEnumerable <PhotoWork.Models.Photographer>

@{
    ViewBag.Title = "SearchByPhotographer";
    if (Session["ROLE"] == null)
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
    else if (String.Compare(Session["ROLE"].ToString(), "client") == 0)
    {
        Layout = "~/Views/Shared/_ClientLayout.cshtml";
    }
}

<div class="container" style="margin-top:50px">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-3">
                <div class="service-form">
                    <img src="~/Content/img/01.jpg" class="img-service" />
                    <div class="photographer-pro">
                        <img src="~/Content/img/01.jpg" class="avatar" />
                        @Html.DisplayFor(modelItem => item .FullName)
                        <p> @Html.DisplayFor(modelItem => item.Bio)</p>
                       
                    </div>
                </div>
            </div>
        }
    </div>
</div>